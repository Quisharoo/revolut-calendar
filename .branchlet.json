{
  "worktreeCopyPatterns": [".env*", ".vscode/**"],
  "worktreeCopyIgnores": ["**/node_modules/**", "**/dist/**", "**/.git/**", "**/.pnpm-store/**"],
  "worktreePathTemplate": "$BASE_PATH-$BRANCH_NAME",
  "postCreateCmd": ["pnpm install --frozen-lockfile", "pnpm db:push"],
  "terminalCommand": "code .",
  "deleteBranchWithWorktree": true
}
